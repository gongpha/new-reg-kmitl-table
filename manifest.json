{
  "name": "KMITL New Timetable",
  "description": "Generate a new rendered timetable section on the registration page on https://new.reg.kmitl.ac.th",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["webRequest"],
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://new.reg.kmitl.ac.th/reg/*"],
      "css": ["style.css"],
      "js": ["content.js"]
    }
  ]
}
